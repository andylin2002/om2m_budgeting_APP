    {
        "id": "530f9bd80d58d342",
        "type": "function",
        "z": "4fec910722902f4d",
        "name": "person function 3",
        "func": "var lines = msg.payload;  // 直接是字串陣列\nvar total = 0;\nvar outputLines = [];\n\nfor (var i = 0; i < lines.length; i++) {\n    var line = lines[i];\n    var match = line.match(/\\$(\\d+),/);\n    if (match && match[1]) {\n        total += parseInt(match[1]);\n        var formatted = line.replace(/^.*: /, \"\");\n        outputLines.push(formatted);\n    }\n}\n\nmsg.payload = \"Total: $\" + total + \"\\n\" + outputLines.join(\"\\n\");\nmsg.headers = { \"Content-Type\": \"text/plain\" };\nreturn msg;\n",
